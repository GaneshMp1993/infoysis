<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">
           
          
      
    <bean class="com.project.wallet.Wallet" id="walletID">
  <property name="id" value="101"></property>
  <property name="color" value="Red"></property>
  <property name="brand" value="Woodland"></property>
  <property name="price" value="2000"></property>
  <property name="material" ref="meterialID"></property>  
  <property name="atmCard" ref="atmCardID"></property>
  <property name="money" ref="moneyID"></property>
  <property name="photo" ref="photoID"></property>
  </bean>
  
  
  <bean class="com.project.wallet.Material" id="meterialID">
  <property name="size" value="2.5"></property>
  <property name="type" value="leather"></property>
  <property name="thickness" value="2.3"></property>
  <property name="price" value="800"></property>
  </bean>
  
  <bean class="com.project.wallet.ATMCard" id="atmCardID">
  <property name="cardNo" value="5464544543334"></property>
  <property name="cvv" value="344"></property>
  <property name="color" value="red"></property>

  </bean>
  <bean class="com.project.wallet.Money" id="moneyID">
  <property name="type" value="coin"></property>
  <property name="size" value="2000"></property>

  </bean>
  <bean class="com.project.wallet.Photo" id="photoID">
  <property name="color" value="blackAndWhite"></property>
  <property name="size" value="2.4"></property>
  
  
  
  
  </bean>
  
  
  
  

</beans>